{
  "name": "atmosinsight",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter web dev",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "tokens": "pnpm --filter tokens build",
    "start": "./start-atmosinsight-monorepo.sh",
    "stop": "./stop-atmosinsight-monorepo.sh",
    "status": "./status-atmosinsight-monorepo.sh",
    "restart": "./stop-atmosinsight-monorepo.sh && ./start-atmosinsight-monorepo.sh",
    "scan": "python3 port_scanner.py",
    "scan:common": "python3 port_scanner.py --common",
    "scan:web": "python3 port_scanner.py --range 80 443",
    "scan:all": "python3 port_scanner.py --all",
    "scan:demo": "./demo_scan.sh",
    "scan:ssh": "python3 port_scanner.py --range 22 22",
    "scan:database": "python3 port_scanner.py --range 3306 5432",
    "scan:local": "python3 port_scanner.py --host 127.0.0.1 --common",
    "kill": "python3 port_killer.py",
    "kill:port": "python3 port_killer.py --port",
    "kill:common": "python3 port_killer.py --common",
    "kill:force": "python3 port_killer.py --force",
    "kill:web": "python3 port_killer.py --port 80,443,8080,8443",
    "kill:database": "python3 port_killer.py --port 3306,5432,27017",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "ci": "pnpm ci --frozen-lockfile --prefer-offline",
    "ci:clean": "rm -rf node_modules && rm -rf */node_modules && rm -rf */*/node_modules && pnpm install --frozen-lockfile",
    "ci:fresh": "rm -rf node_modules && rm -rf */node_modules && rm -rf */*/node_modules && rm -rf .pnpm && pnpm install --frozen-lockfile",
    "ci:script": "./scripts/ci.sh"
  },
  "dependencies": {
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@types/nock": "^11.1.0",
    "@types/node": "^24.3.0",
    "nock": "^14.0.10",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
