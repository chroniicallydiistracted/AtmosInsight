{
  "version": 8,
  "glyphs": "https://protomaps.github.io/basemaps/fonts/{fontstack}/{range}.pbf",
  "sources": {
    "protomaps": {
      "type": "vector",
      "url": "pmtiles://https://pmtiles.io/2018-10-30-sf.pmtiles"
    }
  },
  "layers": [
    { "id": "background", "type": "background", "paint": {"background-color": "#ddeeff"} },
    { "id": "water", "type": "fill", "source": "protomaps", "source-layer": "water", "paint": {"fill-color": "#a0c8f0"} },
    { "id": "landuse", "type": "fill", "source": "protomaps", "source-layer": "landuse", "paint": {"fill-color": "#e0e0e0"} },
    { "id": "roads", "type": "line", "source": "protomaps", "source-layer": "roads", "paint": {"line-color": "#ffffff", "line-width": 1} },
    { "id": "admin", "type": "line", "source": "protomaps", "source-layer": "boundaries", "paint": {"line-color": "#808080", "line-width": 1} },
    { "id": "labels", "type": "symbol", "source": "protomaps", "source-layer": "places", "layout": {"text-field": "{name}", "text-size": 12}, "paint": {"text-color": "#000"} }
  ]
}
